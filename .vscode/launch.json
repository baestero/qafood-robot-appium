{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Depurador do Python: Arquivo Atual",
      "type": "debugpy",
      "request": "launch",
      "program": "${file}",
      "console": "integratedTerminal"
    },

    {
      "type": "robotframework-lsp",
      "name": "Robot Framework: Launch .robot file",
      "request": "launch",
      "cwd": "^\"\\${workspaceFolder}\"",
      "target": "^\"\\${file}\"",
      "terminal": "integrated",
      "env": {},
      "args": []
    }
  ]
}
